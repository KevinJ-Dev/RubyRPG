<h1>Nouvelle quête</h1>
<%= form_with(model: @quest, url: quests_path) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.label :reward %>
    <%= form.text_field :reward %>
  </div>
  <div>
    <%= form.submit "Ajouter la quête" %>
  </div>
<% end %>

